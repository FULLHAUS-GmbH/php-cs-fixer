{
    "name": "fullhaus/php-cs-fixer",
    "description": "Default PHP-CS-Fixer configuration for FULLHAUS projects.",
    "type": "library",
    "homepage" : "https://www.fullhaus.de/",
    "readme": "README.md",
    "license": "MIT",
    "require": {
        "friendsofphp/php-cs-fixer": "^3.68.1",
        "php": ">=8.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.45"
    },
    "autoload": {
        "psr-4": {
            "FULLHAUS\\CodingStandards\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "lock": false
    }
}
